{
  "name": "backup-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": {
    "backup-service": "./index.js"
  },
  "scripts": {
    "dev": "nodemon index.js",
    "generate:schema": "node generateModel.js",
    "update:schema": "npx prisma migrate dev",
    "reset:schema": "npx prisma migrate reset",
    "studio": "npx prisma studio",
    "build": "pkg . --targets node16-win-x64 --output ./build/backup-service.exe --no-bytecode",
    "audit": "npm audit",
    "test": "jest"
  },
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "assets": [
      "config.yaml",
      "node_modules/@prisma/engines/*",
      "node_modules/.prisma/client/*",
      "prisma/schema.prisma",
      "prisma/migrations/*",
      "db/*.db"
    ],
    "outputPath": "build"
  },
  "keywords": [
    "backup",
    "file-management",
    "nodejs",
    "prisma"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "archiver": "^7.0.1",
    "async": "^3.2.5",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "minimatch": "^9.0.4",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.1.0",
    "pkg": "^5.8.1",
    "prisma": "^5.12.1",
    "sqlite": "^5.1.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0"
  }
}
